    "dayjs": "^1.11.7",
    "github-slugger": "^2.0.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.216.0",
    "next": "^13.4.2",
    "notion-client": "^6.16.0",
    "notion-utils": "^6.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
