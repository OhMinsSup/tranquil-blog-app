{
  "name": "tranquil-blog",
  "version": "0.0.1-beta.0",
  "description": "my own silent blog",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "dev:run": "run-s dev:node:*",
    "dev:node:cli": "node dist/cli.js --env local",
    "dev:node:start": "next",
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "fix": "run-p fix:*",
    "fix:types": "tsc --noEmit",
    "fix:eslint-fix": "next lint --fix",
    "fix:prettier-fix": "prettier --write '**/*.{js,jsx,md,ts,tsx}'",
    "cli:start": "tsup --entry.cli scripts/cli/index.ts --minify",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:ci": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "vercel:build": "vercel build",
    "pages:build": "npx @cloudflare/next-on-pages --experimental-minify",
    "pages:deploy": "npm run pages:build && wrangler pages publish .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=\"2023-05-12\" --compatibility-flags=\"nodejs_compat\""
  },
  "keywords": [
    "blog",
    "tranquil",
    "lalossol"
  ],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "extends @anansi/browserslist-config"
  ],
  "dependencies": {
    "@builder.io/partytown": "^0.8.0",
    "@cloudflare/pages-plugin-vercel-og": "^0.1.0",
    "@notionhq/client": "^2.2.5",
    "@radix-ui/react-label": "^2.0.2",
    "@tanstack/query-core": "^4.29.11",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-query-devtools": "^4.29.12",
    "@trpc/client": "^10.29.1",
    "@trpc/next": "^10.29.1",
    "@trpc/react-query": "^10.29.1",
    "@trpc/server": "^10.29.1",
    "class-variance-authority": "^0.6.0",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.8",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.236.0",
    "next": "^13.4.4",
    "notion-to-md": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-stringify": "^9.0.3",
    "remark-breaks": "^3.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "server-only": "^0.0.1",
    "superjson": "^1.12.3",
    "unified": "^10.1.2",
    "zod": "^3.21.4",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@anansi/browserslist-config": "^1.4.3",
    "@cloudflare/next-on-pages": "^1.0.1",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@playwright/test": "^1.34.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/react": "^14.0.0",
    "@types/cookie": "^0.5.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "chalk": "^5.2.0",
    "commander": "^10.0.1",
    "dotenv": "^16.1.4",
    "encoding": "^0.1.13",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^7.0.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.5",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.31.4",
    "wrangler": "^3.0.1"
  }
}
